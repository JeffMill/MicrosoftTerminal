{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "globals": {
        "alwaysShowTabs": true,
        "defaultProfile": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
        "initialCols": 120,
        "initialRows": 30,
        "keybindings": [
            {
                "command": "newTab",
                "keys": [
                    "ctrl+shift+t"
                ]
            },
            // New Window shift+ctrl+n
            {
                "command": "closePane",
                "keys": [
                    "ctrl+shift+w"
                ]
            },
            {
                "command": "closeWindow",
                "keys": [
                    "ctrl+shift+q"
                ]
            },
            {
                "command": "copy",
                "keys": [
                    "ctrl+shift+c"
                ]
            },
            {
                "command": "paste",
                "keys": [
                    "ctrl+shift+v"
                ]
            },
            {
                "command": "toggleFullscreen",
                "keys": [
                    "f11"
                ]
            },
            // Zoom In Ctrl++
            // Zoom Out Ctrl+-
            // Normal Size Ctrl+0
            {
                "command": "find",
                "keys": [
                    "ctrl+shift+f"
                ]
            },
            // Find Next Shift+Ctrl+G
            // Find Previous Shift+Ctrl+H
            // Clear Highlight Shift+Ctrl+J
            {
                "command": "prevTab",
                "keys": [
                    "ctrl+pgup"
                ]
            },
            {
                "command": "nextTab",
                "keys": [
                    "ctrl+pgdn"
                ]
            },
            // Move Tab to the Left: Shift+Ctrl+PgUp
            // Move Tab to the Right: Shift+Ctrl+PgDn
            {
                "command": "switchToTab0",
                "keys": [
                    "alt+1"
                ]
            },
            {
                "command": "switchToTab1",
                "keys": [
                    "alt+2"
                ]
            },
            {
                "command": "switchToTab2",
                "keys": [
                    "alt+3"
                ]
            },
            {
                "command": "switchToTab3",
                "keys": [
                    "alt+4"
                ]
            },
            {
                "command": "switchToTab4",
                "keys": [
                    "alt+5"
                ]
            },
            {
                "command": "switchToTab6",
                "keys": [
                    "alt+7"
                ]
            },
            {
                "command": "switchToTab7",
                "keys": [
                    "alt+8"
                ]
            },
            {
                "command": "switchToTab8",
                "keys": [
                    "alt+9"
                ]
            },
            {
                "command": "scrollUp",
                "keys": [
                    "ctrl+shift+up"
                ]
            },
            {
                "command": "scrollDown",
                "keys": [
                    "ctrl+shift+down"
                ]
            },
            {
                "command": "scrollUpPage",
                "keys": [
                    "ctrl+shift+pgup"
                ]
            },
            {
                "command": "scrollDownPage",
                "keys": [
                    "ctrl+shift+pgdn"
                ]
            }
            // Scroll to top shift+home
            // Scroll to bottom shift+end
        ],
        "requestedTheme": "system",
        "showTabsInTitlebar": true,
        "showTerminalTitleInTitlebar": true,
        "wordDelimiters": " ./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}~?â”‚"
    },
